<VBox alignment="CENTER" prefHeight="183.0" prefWidth="212.0" spacing="20.0" style="-fx-background-color: #FEF7EF;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.kurs.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
    <Button fx:id="hellobut" onAction="#onHelloButtonClick" style="-fx-background-color: #f3625d;" text="Розпочнемо" textFill="WHITE">
      <font>
         <Font size="16.0" />
      </font></Button>
</VBox>
